{
  "name": "npm-workspace",
  "description": "A cli utility to simplify link-ing and install-ing of npm packages locally.",
  "version": "0.7.0",
  "homepage": "https://github.com/barrett-vegas-com/npm-workspace",
  "author": {
    "name": "Mario Casciaro"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/barrett-vegas-com/npm-workspace"
  },
  "bugs": {
    "url": "https://github.com/barrett-vegas-com/npm-workspace/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/mariocasciaro/npm-workspace/blob/master/LICENSE"
    }
  ],
  "scripts": {
    "test": "node_modules/mocha/bin/_mocha test/*.js -t 30000 --reporter spec",
    "compile": "babel src --out-dir lib",

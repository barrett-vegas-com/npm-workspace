  },
  "bin": {
    "npm-workspace": "bin/npm-workspace"
  },
  "preferGlobal": true,
  "dependencies": {
    "commander": "^2.5.0",
    "lodash": "^3.10.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.0.1",
    "ncp": "^2.0.0",
    "rimraf": "^2.2.8",
    "spawned": "^0.1.3",
    "through2": "^2.0.0",
    "when": "^3.5.2"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-plugin-react": "^3.15.0",
    "mocha": "^2.0.1"
  },
  "keywords": [
    "npm",
    "link",
    "dependencies",
    "peerDependencies",
    "workspace",
    "automatic"
  ]
